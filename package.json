    "vue-loader": "^13.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^7.1.2",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "express": "^4.16.3",
    "file-loader": "^1.1.5",
    "node-sass": "^4.5.0",
    "path": "^0.12.7",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.2.0"
  },
  "main": "config.js",
  "repository": {
