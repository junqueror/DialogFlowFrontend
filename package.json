{
  "name": "dialogflow-web",
  "description": "Dialogflow Progressive Web Application",
  "version": "1.7.0",
  "author": "Ushakov",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "comment-start": "node server.js",
    "heroku-postbuild":
      "npm install --only=dev --no-shrinkwrap && npm run build && node server.js",
    "amazing": "npm install && npm run build && npm run start "
  },
